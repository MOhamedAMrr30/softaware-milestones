          <ul class="personal-zone-data">
            <li>
              <span>21</span>
              <i class="fa-solid fa-ticket"></i>
            </li>
            <li class="username">Mohamed Amr</li>
            <li class="zone-name">nasr city</li>
            <li class="zone-id">12765</li>
          </ul>
        </div>
      </div>
    </div>
    <div class="pay-ticket-sub">
      <div class="container">
        <form
          class="pay-ticket-sub-form"
          action="user-dashboard.html"
          method="post"
        >
          <div class="subID">
            <label for="subID">Subscription ID</label>
            <input type="text" name="subID" id="subID" required />
          </div>
          <div class="origin">
            <label for="origin">origin</label>
            <input
              type="text"
              name="origin"
              id="origin"
              readonly
              value="station1"
            />
          </div>
          <div class="destination">
            <label for="destination">destination</label>
            <input
              type="text"
              name="destination"
              id="destination"
              readonly
              value="station2"
            />
          </div>
          <div class="trip-date">
            <label for="trip-date">trip date</label>
            <input
              type="text"
              name="trip-date"
              id="trip-date"
              readonly
              value="17-09-2023"
            />
          </div>
          <input type="submit" value="Book" />
        </form>
        <div class="subscriptions">
          <div class="subscriptions-table">
            <table class="table table-striped">
              <caption>
                Mohamed's Subscriptions
              </caption>
              <thead>
                <tr>
                  <th scope="col">Subscription ID</th>
                  <th scope="col">Subscription Type</th>
                  <th scope="col">Zone ID</th>
                  <th scope="col">User ID</th>
                  <th scope="col">Number of tickets</th>
                </tr>
              </thead>
              <tbody>
                <tr>
                  <td>1</td>
                  <td>Monthly</td>
                  <td>12756</td>
                  <td>132</td>
                  <td>30</td>
                </tr>
                <tr>
                  <td>2</td>
                  <td>Annual</td>
                  <td>12756</td>
                  <td>132</td>
                  <td>200</td>
                </tr>
                <tr>
                  <td>3</td>
                  <td>Quarter</td>
                  <td>12756</td>
                  <td>132</td>
                  <td>90</td>
                </tr>
              </tbody>
            </table>
          </div>
        </div>
      </div>
    </div>

    <!-- jQuery library -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.4/jquery.min.js"></script>

    <!-- Latest compiled JavaScript -->
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/js/bootstrap.min.js"></script>
  </body>
</html>
